https://omegaup.com/arena/problem/el-olvidadizo/#problems


#include <iostream>
#include <string>
using namespace std;

int main()
{
    string frase;
    int contador = 0;
    getline(cin, frase);
    for (int i = 0; i < frase.length(); i++)
    {
        if (frase[i] != ' ')
        {
            contador++;
        }
    }
    cout <<contador-1 <<endl;
    return 0;
}

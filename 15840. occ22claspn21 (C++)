#include <iostream>
#include <string>
using namespace std;

int main() {
    int n;
    cin >> n;
    for (int i = 0; i < n; i++) {
        int n1;
        char a1;
        cin >> n1 >> a1;
        for (int j = 0; j < n1; j++) {
            cout << a1;
        }
    }
    return 0;
}

https://omegaup.com/arena/problem/occ22sfinpn3/#problems


#include <iostream>
using namespace std;

int main() {
    int x1, y1, x2, y2;
    cin >> x1 >> y1 >> x2 >> y2;
    if (x1 == x2) {
        cout << "vertical" << endl;
    } else if (y1 == y2) {
        cout << "horizontal" << endl;
    } else {
        cout << "ninguna" << endl;
    }
    return 0;
}
